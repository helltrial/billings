{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Acks": "all",
    "Retries": 5,
    "EnableIdempotence": true,
    "CompressionType": "gzip"
  }

}
